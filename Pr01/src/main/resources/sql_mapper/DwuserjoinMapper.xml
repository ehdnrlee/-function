<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.pro.dao.DwUserDao">
	<insert id="joinuser" parameterType="string">
		insert into users values(#{id},#{pwd},#{irum},#{pNumber},#{email})
	</insert>
	<select id="selectPwd" parameterType="String" resultType="String">
		select pwd from users where id=#{id};
	</select>
	<select id="getUserInfo" parameterType="String" resultType="user">
		select * from users where id=#{id}
	</select>
	
</mapper>